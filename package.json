{
    "name": "back-office",
    "version": "1.0.0",
    "author": "Subash",
    "description": "Point Of Sales Desktop App using Angular11 + Electron + NodeJs + MySQL",
    "main": "electron-main.js",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build --prod --base-href",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "electron-start": "electron .",
        "build-new": "electron-packager . BackOffice --platform=win32 --arch=ia32",
        "build-mac": "ng build --prod --base-href ./ && electron-packager . --overwrite --ignore=(\"e2e|node_modules|src|.browserslistrc|.editorconfig|.gitignore|angular.json|karma.conf.js|package-lock.json|README.md|tsconfig.app.json|tsconfig.json|tsconfig.spec.json|tslint.json\") --platform=darwin --arch=x64 --icon=assets/icons/mac/logo.ico --prune=true --out=./dist/electron-build",
        "build-win": "ng build --prod --base-href ./ && electron-packager . BackOffice --overwrite --ignore=(\"e2e|node_modules|src|.browserslistrc|.editorconfig|.gitignore|angular.json|karma.conf.js|package-lock.json|README.md|tsconfig.app.json|tsconfig.json|tsconfig.spec.json|tslint.json\") --asar --platform=win32 --arch=ia32 --icon=./dist/angular-build/assets/icons/win/logo.ico --prune=true --out=./dist/electron-build --version-string.CompanyName=DevSociety --version-string.FileDescription=POS_SOFTWARE --version-string.ProductName='POS_BACKOFFICE_APP'",
        "build-linux": "ng build --prod --base-href ./ && electron-packager . BackOffice --overwrite --ignore=(\"e2e|node_modules|src|.browserslistrc|.editorconfig|.gitignore|angular.json|karma.conf.js|package-lock.json|README.md|tsconfig.app.json|tsconfig.json|tsconfig.spec.json|tslint.json\") --asar --platform=win32 --arch=ia32 --icon=./dist/angular-build/assets/icons/win/logo.ico --prune=true --out=./dist/electron-build --version-string.CompanyName=DevSociety --version-string.FileDescription=POS_SOFTWARE --version-string.ProductName='POS_BACKOFFICE_APP'"
    },
    "private": true,
    "browser": {
        "fs": false,
        "path": false,
        "os": false
    },
    "dependencies": {
        "@angular/animations": "~11.2.1",
        "@angular/cdk": "^11.2.1",
        "@angular/common": "~11.2.1",
        "@angular/compiler": "~11.2.1",
        "@angular/core": "~11.2.1",
        "@angular/forms": "~11.2.1",
        "@angular/material": "^11.2.1",
        "@angular/platform-browser": "~11.2.1",
        "@angular/platform-browser-dynamic": "~11.2.1",
        "@angular/router": "~11.2.1",
        "angular-material-keyboard": "0.0.9",
        "electron-packager": "^15.2.0",
        "electron-pos-printer": "^1.2.0",
        "escpos-usb": "^3.0.0-alpha.4",
        "file-saver": "^2.0.5",
        "filesaver": "0.0.13",
        "font-awesome": "^4.7.0",
        "jspdf": "^2.3.1",
        "jspdf-autotable": "^3.5.14",
        "ng2-slim-loading-bar": "^4.0.0",
        "primeflex": "^2.0.0",
        "primeicons": "^4.1.0",
        "primeng": "^11.2.3",
        "quagga": "^0.12.1",
        "rxjs": "~6.6.0",
        "rxjs-compat": "^6.6.3",
        "simple-keyboard": "^2.32.131",
        "tslib": "^2.0.0",
        "usb-barcode-scanner": "^1.0.2",
        "xlsx": "^0.16.9",
        "zone.js": "~0.11.3"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.1102.1",
        "@angular/cli": "~11.2.1",
        "@angular/compiler-cli": "~11.2.1",
        "@types/jasmine": "~3.6.0",
        "@types/node": "^12.11.1",
        "codelyzer": "^6.0.0",
        "electron": "^9.1.0",
        "jasmine-core": "~3.6.0",
        "jasmine-spec-reporter": "~5.0.0",
        "karma": "~6.1.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage": "~2.0.3",
        "karma-jasmine": "~4.0.0",
        "karma-jasmine-html-reporter": "^1.5.0",
        "protractor": "~7.0.0",
        "ts-node": "~8.3.0",
        "tslint": "~6.1.0",
        "typescript": "~4.1.2"
    }
}
